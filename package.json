{
    "name": "bobrilstrap",
    "version": "3.4.1",
    "description": "Bobril wrapper for bootstrap",
    "author": "Tomáš Růt",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/keeema/bobrilstrap/issues"
    },
    "homepage": "http://keeema.github.io/bobrilstrap",
    "repository": "https://github.com/keeema/bobrilstrap.git",
    "keywords": [
        "bootstrap",
        "bobril",
        "virtual-dom",
        "components"
    ],
    "typescript": {
        "main": "index.ts"
    },
    "bobril": {
        "example": "example.ts",
        "title": "Bobrilstrap",
        "additionalResourcesDirectory": "example/assets",
        "head": "<meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><link rel=\"shortcut icon\" type=\"image/ico\" href=\"favicon.ico\" /><link rel=\"manifest\" href=\"manifest.json\"><meta name=\"apple-mobile-web-app-capable\" content=\"yes\"><meta name=\"apple-mobile-web-app-status-bar-style\" content=\"#6f5499\"><meta name=\"apple-mobile-web-app-title\" content=\"Bobrilstrap\"><link rel=\"apple-touch-icon\" href=\"/icons/icon-152x152.png\"><meta name=\"description\" content=\"Bobril wrapper of the most popular HTML, CSS, and JS framework for developing responsive, mobile first projects on the web - Bootstrap.\"><meta name=\"theme-color\" content=\"#6f5499\" />",
        "compilerOptions": {
            "declaration": true,
            "strict": true,
            "noUnusedLocals": true,
            "noUnusedParameters": true,
            "noImplicitReturns": true,
            "noFallthroughCasesInSwitch": true,
            "strictPropertyInitialization": true,
            "preserveConstEnums": false,
            "removeComments": false,
            "sourceMap": true
        }
    },
    "prettier": {
        "printWidth": 140,
        "tabWidth": 4
    },
    "dependencies": {
        "@types/bootstrap": "^3.3.35",
        "@types/bootstrap-3-typeahead": "^4.0.1",
        "@types/jquery": "^3.3.36",
        "bobril": "^14.6.0",
        "bootstrap": "^3.4.0",
        "bootstrap-3-typeahead": "^4.0.2",
        "jquery": "^3.5.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-bobril": "^1.0.2",
        "prettier": "^2.0.5",
        "pwa-install-handler": "^2.0.6",
        "typescript": "^3.8.3"
    }
}
