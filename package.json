{
    "name": "bobrilstrap",
    "version": "4.0.0",
    "description": "Bobril wrapper for bootstrap 4.5",
    "author": "Tomáš Růt",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/keeema/bobrilstrap/issues"
    },
    "homepage": "http://keeema.github.io/bobrilstrap",
    "repository": "https://github.com/keeema/bobrilstrap.git",
    "keywords": [
        "bootstrap",
        "bobril",
        "virtual-dom",
        "components"
    ],
    "typescript": {
        "main": "index.ts"
    },
    "bobril": {
        "buildOutputDir": "www",
        "example": "example/example.tsx",
        "title": "Bobrilstrap 4",
        "additionalResourcesDirectory": "example/assets",
        "head": "<meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><link rel=\"shortcut icon\" type=\"image/ico\" href=\"favicon.ico\" />",
        "compilerOptions": {
            "declaration": true,
            "strict": true,
            "noUnusedLocals": true,
            "noUnusedParameters": true,
            "noImplicitReturns": true,
            "noFallthroughCasesInSwitch": true,
            "strictPropertyInitialization": true,
            "preserveConstEnums": false,
            "removeComments": false,
            "sourceMap": true
        }
    },
    "prettier": {
        "printWidth": 140,
        "tabWidth": 4
    },
    "dependencies": {
        "@types/bootstrap": "^4.5.0",
        "bobril": "^14.8.0",
        "bootstrap": "^4.5.0"
    },
    "devDependencies": {
        "@popperjs/core": "^2.4.0",
        "@types/jquery": "^3.3.38",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-bobril": "^1.0.2",
        "i": "^0.3.6",
        "jquery": "^3.5.1",
        "npm": "^6.14.5",
        "prettier": "^2.0.5",
        "pwa-install-handler": "^2.0.6",
        "typescript": "^3.9.2"
    }
}
