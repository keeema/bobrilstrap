{
  "name": "bobrilstrap",
  "version": "3.0.5",
  "description": "Bobril wrapper for bootstrap",
  "author": "Tomáš Růt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keeema/bobrilstrap/issues"
  },
  "homepage": "http://keeema.github.io/bobrilstrap",
  "repository": "https://github.com/keeema/bobrilstrap.git",
  "keywords": [
    "bootstrap",
    "bobril",
    "virtual-dom",
    "components"
  ],
  "typescript": {
    "main": "index.ts"
  },
  "bobril": {
    "example": "example.ts",
    "title": "Bobrilstrap",
    "additionalResourcesDirectory": "example/assets",
    "head": "<meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><link rel=\"shortcut icon\" type=\"image/ico\" href=\"favicon.ico\" />",
    "compilerOptions": {
      "declaration": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noEmitHelpers": true,
      "preserveConstEnums": false,
      "removeComments": false,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "sourceMap": true
    }
  },
  "dependencies": {
    "@types/bootstrap": "^3.3.35",
    "@types/jquery": "^3.3.29",
    "bobril": "^10.1.2",
    "bootstrap": "^3.4.0",
    "bootstrap-3-typeahead": "^4.0.2",
    "jquery": "^3.3.1"
  }
}
